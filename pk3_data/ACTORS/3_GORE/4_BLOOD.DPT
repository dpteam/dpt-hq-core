ACTOR WALLREDBLOOD
{
	SCALE 0.6
	SPEED 7
	HEALTH 1
	RADIUS 8
	HEIGHT 1
	GRAVITY 0.7
	DAMAGE 0
	DAMAGETYPE BLOOD
	DECAL BRUTALBLOODSUPER
	+MISSILE
	+CLIENTSIDEONLY
	+NOTELEPORT
	+NOBLOCKMAP
	+BLOODLESSIMPACT 
	+RIPPER
	STATES
	{
SPAWN:
		TNT1 A 0 A_JUMPIF(WATERLEVEL > 1, "SPLASH")
		TNT1 ABCD 4
		LOOP
SPLASH:
		BLOD A 0
		STOP
DEATH:
		TNT1 A 3
		STOP
	}
}
