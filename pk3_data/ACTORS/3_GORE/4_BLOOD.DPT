ACTOR TINYBLOOD
{
	SCALE 0.2
	SPEED 7
	HEALTH 1
	RADIUS 8
	HEIGHT 1
	+DONTSPLASH
	GRAVITY 0.7
	DECAL BRUTALBLOODSPLAT
	+MISSILE
	+CLIENTSIDEONLY
	+NOTELEPORT
	+NOBLOCKMAP
	+FORCEXYBILLBOARD
	-EXPLODEONWATER
	+DOOMBOUNCE
	BOUNCEFACTOR 0.01
	STATES
	{
