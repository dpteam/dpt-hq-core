ACTOR Guest : PlayerPawn
{
	DamageFactor "PrisonPower", 100.0
	Speed 1
	Health 100
	Radius 16
	Height 56
	Mass 100
	PainChance 255
	Player.DisplayName "DPT.HQ Guest"
	Player.SoundClass "guest"
	Player.StartItem "RivetGun"
	Player.StartItem "Unarmed"
	Player.StartItem "Dick"
	Player.StartItem "SatchelAmmo", 5
	Player.StartItem "Satcheler"
	Player.StartItem "BlueSprayPaint"
	Player.StartItem "RedSprayPaint"
	Player.StartItem "GreenSprayPaint"
	Player.StartItem "SprayPaint"
	Player.StartItem "Rivets", 225
	Player.StartItem "Stamina", 999
	Player.StartItem "HungryLevel", 999
	Player.StartItem "Cash", 999
	Player.StartItem "Artipoisonbag", 25
	Player.ColorRange 112, 127
	States
	{
	Spawn:
		ELIT A 1
		ELIT A 0 A_JumpIfInventory("DrunkLevel", 40, "Spawn_Drunk")
		Loop
	Spawn_Drunk:
		ELIT A 0 A_JumpIf (momx > 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momy > 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momx < 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momy < 0, "See_Drunk")
		ELIT A 20
		ELIT A 0 A_Jump(255, "SpD_1", "SpD_2", "SpD_3")
		Goto SpD_3
	SpD_1:
		ELIT A 0 A_JumpIf (momx > 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momy > 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momx < 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momy < 0, "See_Drunk")
		ELIT A 1 A_SetPitch(-0.20)
		ELIT A 1 A_SetPitch(0.20)
		ELIT A 1 A_SetPitch(0.30)
		ELIT A 1 A_SetPitch(-0.42)
		Goto Spawn_Drunk
	SpD_2:
		ELIT A 0 A_JumpIf (momx > 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momy > 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momx < 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momy < 0, "See_Drunk")
		ELIT A 1 A_SetPitch(-0.30)
		ELIT A 1 A_SetPitch(-0.30)
		ELIT A 1 A_SetPitch(-0.30)
		ELIT A 1 A_SetPitch(-0.30)
		Goto Spawn_Drunk
	SpD_3:
		ELIT A 0 A_JumpIf (momx > 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momy > 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momx < 0, "See_Drunk")
		ELIT A 0 A_JumpIf (momy < 0, "See_Drunk")
		ELIT A 1 A_SetPitch(0.30)
		ELIT A 1 A_SetPitch(0.30)
		ELIT A 1 A_SetPitch(0.30)
		ELIT A 1 A_SetPitch(0.30)
		ELIT A 1 A_SetAngle(random(-angle-15,angle+15))
		Goto Spawn_Drunk
	See:
        ELIT AB 4 A_JumpIfInventory("DrunkLevel", 40, "See_Drunk")
		ELIT A 0 A_SpawnItem("Footstep",30,0.001)
		ELIT CD 4 A_JumpIfInventory("DrunkLevel", 40, "See_Drunk")
		ELIT A 0 A_SpawnItem("Footstep",30,0.001)
		Loop
	See_Drunk:
        ELIT AB 4 A_SetPitch(-0.30)
		ELIT CD 4 A_SetPitch(0.50)
        ELIT AB 4 A_SetPitch(-0.20)
		ELIT CD 4 A_SetPitch(0.25)
		ELIT A 0 A_Jump(26, "Vomit")
        ELIT AB 4 A_SetPitch(-0.90)
		ELIT CD 4 A_SetAngle(random(angle-5,angle+5))
		Goto Spawn_Drunk
	Vomit:
		ELIT GGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGGG 1 A_Stop
		ELIT G 0 A_PlaySound("player/vomit", 2)
		ELIT GGGGGGGGGG 1 A_Stop
		ELIT G 0 A_SetPitch(-0.85)
		ELIT GGGGGGGG 2 A_SpawnItemEx("PlayerVomit", 6, random(-8, 8), 32, random(6, 18))
		ELIT A 0 A_takeinventory("DrunkLevel",20)//Half of the drunk line are lost
		ELIT GGGGGGGG 1 A_Stop
		Goto Spawn_Drunk
	Missile:
		ELIT A 0 A_JumpIfInventory("DrunkLevel", 40, "Missile_Drunk")
		ELIT E 12 
		Goto Spawn
	Missile_Drunk:
		ELIT A 0 A_SetAngle(random(angle-12,angle+12))
		ELIT EEEEEE 2 A_SetAngle(random(angle-4,angle+4))
		Goto Spawn_Drunk
	Melee:
		ELIT A 0 A_JumpIfInventory("DrunkLevel", 40, "Melee_Drunk")
		ELIT F 6 BRIGHT
		Goto Missile
	Melee_Drunk:
		ELIT FFFFFF 1 BRIGHT A_SetPitch(0.10)
		Goto Missile_Drunk
	Pain.Jail:
		ELIT A 0 ACS_ExecuteAlways(700, 0)
		Goto Spawn
	Pain.Fire:
		ELIT G 4 
		ELIT G 4 A_Playsound ("*burnpain")// A_Playsound("bodysplat")
		Goto Spawn
	Pain:
		ELIT G 4 
		ELIT G 4 A_Pain
		Goto Spawn
	Pain_Drunk:
		ELIT A 0 A_JumpIfInventory("DrunkLevel", 40, "Pain_Drunk")
		ELIT G 4 A_SetPitch(0.75)
		ELIT G 4 A_Pain
		Goto Spawn_Drunk
	Death:
		ELIT A 0 A_TakeInventory("DrunkLevel", 100)
		ELIT H 10
		ELIT I 10 A_PlayerScream
		ELIT J 10 A_NoBlocking
		ELIT KLM 10
		ELIT N -1
		Stop
	XDeath:
		ELIT A 0 A_SpawnItemEx("MarineXDeath", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION)
		ELIT A 0 A_TakeInventory("DrunkLevel", 100)
		ELIT O 5
		ELIT P 5 A_XScream
		ELIT Q 5 A_NoBlocking
		ELIT RSTUV 5
		ELIT W -1
		Stop
	Burn:
		BURN A 0 A_TakeInventory("DrunkLevel", 100)
        BURN A 3 Bright A_PlaySound("*burndeath")
        BURN B 3 Bright
        BURN C 3 Bright
        BURN D 3 Bright A_NoBlocking
        BURN E 5 Bright
        BURN FGH 5 Bright
        BURN I 5 Bright
        BURN JKL 5 Bright
        BURN M 5 Bright
        BURN N 5 Bright
        BURN OPQPQ 5 Bright
        BURN RSTU 7 Bright
        BURN V -1
        Stop
	}
}

