		TNT1 A 0 A_SpawnItemEx ("TankTurretNotReal" , 0, 0, 0, 0, 0, 0, 0, SXF_ABSOLUTEVELOCITY)
		TNT1 A 0 A_SpawnItemEx ("TankBaseNotRealMoving1" , 0, 0, 0, 0, 0, 0, user_tankbasedirection, SXF_ABSOLUTEVELOCITY | SXF_ABSOLUTEANGLE)
